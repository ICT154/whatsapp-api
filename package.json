{
  "name": "wa-gateway",
  "version": "3.2.2",
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run build:css\"",
    "serve": "nodemon index.js",
    "build:css": "tailwindcss -i ./src/style.css -o ./public/css/style.css --watch",
    "build:css:once": "tailwindcss -i ./src/style.css -o ./public/css/style.css --minify",
    "build:css:dev": "tailwindcss -i ./src/style.css -o ./public/css/style.css",
    "start": "node index.js",
    "test:webhook": "node tests/webhook-test.js",
    "test:webhook-server": "node tests/webhook-test-server.js"
  },
  "author": "mimamch <mimamch28@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.18",
    "axios": "^1.11.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "daisyui": "^4.12.10",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "qrcode": "^1.5.1",
    "sharp": "^0.32.6",
    "wa-multi-session": "^3.3.1",
    "wa-sticker-formatter": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mimamch/wa-gateway.git"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.1"
  },
  "description": "Easy Setup Headless multi session Whatsapp Gateway with NodeJS",
  "main": "index.js",
  "keywords": [],
  "bugs": {
    "url": "https://github.com/mimamch/wa-gateway/issues"
  },
  "homepage": "https://github.com/mimamch/wa-gateway#readme"
}
